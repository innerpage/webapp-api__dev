{
  "name": "widgets-api-dev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "clean_old_prod": "rm -rf prod && cd .. && rm -rf prod",
    "move_new_prod": "mv prod ../",
    "copy_config": "cp {.env,.env-blank,.gitignore,package.json} ../prod",
    "prod_init": "cd .. && cd prod && git init && git add --all && git commit -m \"Success\" && git branch -M main && git remote add origin git@github.com-vital-widgets:vital-widgets/widgets-api-prod.git && npm install",
    "pfd": "npm run clean_old_prod && npm run build && npm run move_new_prod && npm run copy_config && npm run prod_init"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-vital-widgets:vital-widgets/widgets-api-dev.git"
  },
  "author": "Tuhin Bhuyan (Projckt)",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "luxon": "^3.2.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.28.0",
    "socket.io": "^4.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/luxon": "^3.2.0",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0"
  }
}
